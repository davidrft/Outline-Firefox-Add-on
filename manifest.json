{
    "description": "Remove the clutter.",
    "manifest_version": 2,
    "name": "Outline",
    "version": "1.0",
  
    "background": {
      "scripts": ["background.js"]
    },
   
    "browser_action": {
      "default_icon": {
        "16": "icons/outline-16.png",
        "32": "icons/outline-32.png",
        "48": "icons/outline-48.png",
        "128": "icons/outline-128.png"
      }
    },

    "permissions": [
        "activeTab"
    ]
}